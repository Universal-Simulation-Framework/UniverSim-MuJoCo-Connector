<mujoco>
    <option timestep="0.001" integrator="RK4" gravity="0 0 -0.1">
        <flag sensornoise="disable" energy="enable" contact="enable" />
    </option>

    <asset>
        <texture name="grid" type="2d" builtin="checker" width="512" height="512" rgb1=".1 .2 .3"
            rgb2=".2 .3 .4" />
        <material name="grid" texture="grid" texrepeat="1 1" texuniform="true" reflectance=".2" />
    </asset>

    <worldbody>
        <geom name="floor" size="0 0 .05" type="plane" material="grid" />
        <light diffuse=".5 .5 .5" pos="0 0 5" dir="0 0 -1" />
    </worldbody>

    <worldbody>
        <body name="box" pos="0 0 .6">
            <freejoint />
            <geom type="box" size=".5 .5 .05" rgba="0 .9 0 1" mass="6" />
            <geom type="box" pos=".45 .45 -.25" size=".05 .05 .25" rgba="0 .9 0 1" mass="1" />
            <geom type="box" pos=".45 -.45 -.25" size=".05 .05 .25" rgba="0 .9 0 1" mass="1" />
            <geom type="box" pos="-.45 .45 -.25" size=".05 .05 .25" rgba="0 .9 0 1" mass="1" />
            <geom type="box" pos="-.45 -.45 -.25" size=".05 .05 .25" rgba="0 .9 0 1" mass="1" />
        </body>
        <body name="bowling" pos="0 0 2">
            <freejoint />
            <geom type="sphere" size=".1 .1 .1" rgba="0 0 .9 1" mass="1" />
        </body>
    </worldbody>
</mujoco>