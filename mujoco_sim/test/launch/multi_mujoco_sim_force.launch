<launch>

    <arg name="robot" default="$(find mujoco_sim)/model/test/bowl.xml" />
    <arg name="world_1" default="$(find mujoco_sim)/model/test/table_with_bowling.xml" />
    <arg name="world_2" default="$(find mujoco_sim)/model/test/bowling.xml" />
    <arg name="sim_1" default="$(find mujoco_sim)/test/config/force_1.yaml" />
    <arg name="sim_2" default="$(find mujoco_sim)/test/config/force_2.yaml" />

    <group ns="sim_1">
        <node name="mujoco_sim" pkg="mujoco_sim" type="mujoco_sim_node" output="screen"
            args="7500">
            <rosparam command="load" file="$(arg sim_1)" />
            <param name="world" value="$(arg world_1)" />
            <param name="robot" type="str" value="$(arg robot)" />
        </node>
    </group>

    <group ns="sim_2">
        <node name="mujoco_sim" pkg="mujoco_sim" type="mujoco_sim_node" output="screen"
            args="7501">
            <rosparam command="load" file="$(arg sim_2)" />
            <param name="world" value="$(arg world_2)" />
            <param name="robot" type="str" value="$(arg robot)" />
        </node>
    </group>

    <!-- <node pkg="universim_server" type="universim_server_node" name="universim_server" output="screen"
    args="7500 7501 7502"/> -->

</launch>